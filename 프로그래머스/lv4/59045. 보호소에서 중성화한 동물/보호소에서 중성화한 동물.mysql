-- 코드를 입력하세요
-- INS에서 Intact먼저 고르고
-- outs에서 spayed, Neutered고르기
SELECT 
# *
OUTS.ANIMAL_ID
, OUTS.ANIMAL_TYPE
# ,LEFT(INS.SEX_UPON_INTAKE,6)
# ,LEFT(OUTS.SEX_UPON_OUTCOME,6)
, OUTS.NAME 
FROM ANIMAL_OUTS OUTS
JOIN ANIMAL_INS INS
ON OUTS.ANIMAL_ID = INS.ANIMAL_ID
WHERE (LEFT(INS.SEX_UPON_INTAKE,6) = 'Intact' AND LEFT(OUTS.SEX_UPON_OUTCOME,6) != 'Intact')
ORDER BY OUTS.ANIMAL_ID